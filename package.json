{
  "name": "resume-maloi",
  "description": "Easer way to write a resume.",
  "version": "0.0.0",
  "main": "app.js",
  "scripts": {
    "ng": "ng",
    "startEle": "ng build --source-map --base-href ./ && electron .",
    "start": "ng serve --host 0.0.0.0 --disable-host-check",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "buildele": "electron-packager . resume-maloi --overwrite --platform linux --arch x64 --out dist_resume/",
    "deb64": "electron-installer-debian --src dist_resume/resume-maloi-linux-x64/ --dest dist_resume/installers/ --arch amd64"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/cdk": "^15.2.6",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/material": "^15.2.6",
    "@angular/material-moment-adapter": "^15.2.6",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "ngx-electron": "^2.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.2",
    "@angular/cli": "~15.2.2",
    "@angular/compiler-cli": "^15.2.0",
    "@types/jasmine": "~4.3.0",
    "electron": "^24.1.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.9.4"
  }
}